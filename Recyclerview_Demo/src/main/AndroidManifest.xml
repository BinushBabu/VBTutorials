<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dev.vbtutorials.rv_demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="dev.vbtutorials.rv_demo.ui.activity.ActivityMain">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="dev.vbtutorials.rv_demo.ui.activity.RecyclerGridActivity"></activity>
        <activity android:name="dev.vbtutorials.rv_demo.ui.activity.RecyclerListActivity"></activity>
        <activity android:name="dev.vbtutorials.rv_demo.ui.activity.RecyclerWithCardActivity"></activity>
        <activity android:name="dev.vbtutorials.rv_demo.ui.activity.RecyclerWithStaggeredGridActivity"></activity>
    </application>

</manifest>